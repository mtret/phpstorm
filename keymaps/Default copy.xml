<keymap version="1" name="Default copy" parent="Default for XWin">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad1" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad2" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad6" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt numpad9" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt numpad7" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt numpad5" />
  </action>
  <action id="Replace">
    <keyboard-shortcut first-keystroke="ctrl r" />
    <keyboard-shortcut first-keystroke="ctrl alt decimal" />
  </action>
  <action id="ShowUsages">
    <keyboard-shortcut first-keystroke="ctrl alt 7" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad3" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>